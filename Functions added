from tkinter import *

tk=Tk()

tk.title("Title Name") #Giving title name
tk.geometry("500x500+0+0") #Setting size and position

a=Label(text="Label Name").place(x=0,y=0) #Label added
b=Button(text="Button").place(x=0,y=20) #Button added
c=Entry().place(x=0,y=50) #Text bar added
#place() - to set position x=X-axis,y=Y-axis
